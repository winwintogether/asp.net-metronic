                                                        <th> Average Price </th>                                                      
                                                    </tr>
                                                </thead>
                                                <tbody></tbody>
                                            </table>
                                        </div>

                                        <div class="col-md-5">

                                            <div id="chart_2" class="chart" style="height: 400px;"> </div>

                                        </div>
                                        
                                    </div>
                                </div>
                                <div class="tab-pane fade  active in" id="tab_2">
                                    <div class="row">
                                        <div class="col-md-12">
                                            <table class="table table-striped table-bordered table-hover" id="table_6">
                                                <thead>
                                                    <tr>
                                                        <th style="display:none"> ID </th>
                                                        <th> Event ID </th>
                                                        <th> Event Title </th>
                                                        <th> Venue </th>
                                                        <th> Event Date </th>
                                                        <th> Zone </th>
                                                        <th> Section </th>
                                                        <th> Row </th>
                                                        <th> Price </th>
                                                        <th> Qty </th>
                                                        <th> Soild Date </th>
                                                    </tr>
                                                </thead>
                                                <tbody></tbody>
                                            </table>
                                        </div>
                                        
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                    <!-- END EXAMPLE TABLE PORTLET-->
                </div>
            </div>
