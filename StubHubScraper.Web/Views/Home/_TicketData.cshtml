    <div class="page-content-wrapper">
        <!-- BEGIN CONTENT BODY -->
        <div class="page-content">
            <div class="row">
                <div class="col-md-12">
                    <div class="portlet light bordered">
                        <div class="portlet-title">
                            <div class="caption">
                                <i class="fa fa-cogs font-green-sharp"></i>
                                <span class="caption-subject font-green-sharp bold uppercase">Ticket Filter Options</span>
                            </div>

                        </div>
                        <div class="portlet-body">
                            <form class="form-horizontal">
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="row">
                                            <div class="form-group">
                                                <label class="col-md-2 control-label" for="title">Search List:</label>
                                                <div class="col-md-3">
                                                    <select id="SearchId" class="form-control input-inline" style="width:100% !important"></select>
                                                </div>
                                            
                                                <label class="col-md-2 control-label" for="title">Event Date:</label>
                                                <div class="col-md-5">
                                                    <input id="StartDate" type="text" class="form-control input-small input-inline date-picker" />
                                                    <span>To:</span>
                                                    <input id="EndDate" type="text" class="form-control input-small input-inline date-picker" />
                                                </div>
                                            </div>    
                                        </div>
                                        <div class="row">
                                            <div class="form-group">
                                                <label class="col-md-2 control-label" for="title">Event:</label>
                                                <div class="col-md-3" >
                                                    <select id="EventId" class="form-control input-inline" style="width:100% !important"></select>
                                                </div>
                                            
                                                <label class="col-md-2 control-label" for="title">Zone:</label>
                                                <div class="col-md-5">
                                                    <input id="Zone" type="text" class="form-control input-large input-inline" />
                                                </div>
                                            </div>
                                        </div>    
                                        <div class="row">
                                            <div class="form-group">
                                                <label class="col-md-2 control-label" for="title">Event Title:</label>
                                                <div class="col-md-3">
                                                    <input id="EventTitle" type="text" class="form-control input-inline" style="width:100% !important"/>
                                                </div>
                                           
                                                <label class="col-md-2 control-label" for="title">Section:</label>
                                                <div class="col-md-5">
                                                    <input id="SectionForm" type="text" class="form-control input-small input-inline" />
                                                    <span>To:</span>
                                                    <input id="SectionTo" type="text" class="form-control input-small input-inline" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="form-group">
                                                <label class="col-md-2 control-label" for="title">Venue:</label>
                                                <div class="col-md-3">
                                                    <input id="EventVenue" type="text" class="form-control input-inline" style="width:100% !important"/>
                                                </div>
                                                <div class="col-md-2"></div>
                                                <div class="col-md-5">
                                                    <div class="md-checkbox">
                                                        <input type="checkbox" id="LastWeekSalesOnly" class="md-check">
                                                        <label for="LastWeekSalesOnly">
                                                            <span></span>
                                                            <span class="check"></span>
                                                            <span class="box"></span>Last Week Sales Only
                                                        </label>
                                                    </div>
                                                    <div class="md-checkbox">
                                                        <input type="checkbox" id="HidePastEvents" class="md-check">
                                                        <label for="HidePastEvents">
                                                            <span></span>
                                                            <span class="check"></span>
                                                            <span class="box"></span>Hide Past Events
                                                        </label>
                                                    </div>
                                                    <div class="md-checkbox">
                                                        <input type="checkbox" id="ShowArchivedSearches" class="md-check">
                                                        <label for="ShowArchivedSearches">
                                                            <span></span>
                                                            <span class="check"></span>
                                                            <span class="box"></span>Show Archieved Searches
                                                        </label>
                                                    </div>
                                                </div>    
                                                
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <button id="btnLookupTickets" type="button" class="btn btn-primary" style="display:block">LookUp Tickets</button>
                                        <br>
                                        <button id="btnExportTicketsToCSV" type="button" class="btn btn-primary" style="display:block">Export Tickets to CSV</button>
                                        <br>
                                        <button id="btnDeleteSelectedTickets" type="button" class="btn btn-primary" style="display:block">Delete Selected Tickets</button>
                                    </div>                                   
                                    
                                </div>
                            </form>
                        </div>
                    </div>
                </div>

                <div class="col-md-12">
                    <!-- BEGIN EXAMPLE TABLE PORTLET-->
                    <div class="portlet light ">
