        </div>
        <!-- END SIDEBAR -->
    </div>
    <!-- END SIDEBAR -->
    <!-- BEGIN CONTENT -->
    <div class="page-content-wrapper">
        <!-- BEGIN CONTENT BODY -->
        <div class="page-content">
            <div class="row">
                <div class="col-md-12">
                    <!-- BEGIN EXAMPLE TABLE PORTLET-->
                    <div class="portlet box green ">
                        <div class="portlet-title">
                            <div class="caption font-white-sharp">
                                <span>
                                    Archived Searches
                                </span>
                            </div>

                        </div>
                        <div class="portlet-body">
                           <div class="row">
                              <div class="col-md-12">
                                  <div class="md-checkbox">
                                      <input type="checkbox" id="showArchivedSearches" class="md-check">
                                      <label for="showArchivedSearches">
                                          <span></span>
                                          <span class="check"></span>
                                          <span class="box"></span> Show Archived Searches
                                      </label>
                                  </div>
                                  <hr> 
                              </div>  
                              <div class="col-md-8">
                                  <table class="table table-striped table-bordered table-hover" id="table_3">
                                     <thead>
                                         <tr>
                                            <th style="display:none"> ID </th>
                                            <th> Name </th>
                                            <th> Schedule Date </th>
                                            <th> Run Once Date </th>
                                            <th> Archived </th>                                          
                                          </tr>
                                       </thead>
                                       <tbody></tbody>
                                   </table>
                                </div>
                              <div class="col-md-4">
                                    <form class="form-horizontal">
                                        <div class="row" style="height:100px;">
                                            <button id="btnReload" type="button" class="btn green">Reload</button> 
                                            <button id="btnDeleteSelectedSearches" type="button" class="btn green">Delete Selected Searches</button> 
                                        </div>
                                     </form>
                               </div>
                            </div>
                        </div>
                    </div>
                    <!-- END EXAMPLE TABLE PORTLET-->
                </div>

                <div class="col-md-12">
                   <div class="portlet box green">
                        <div class="portlet-title">
                            <div class="caption font-white-sharp">
                                <span>
                                    Details
                                </span>
                            </div>

                        </div>
                        <div class="portlet-body">
                           <div class="row">
                               <div class="col-md-8">
                                   <h4>Search Events</h4>
                                   <div class="row">
                                       <div class="col-md-1"><button id="btnDeleteSearchEvent" type="button" class="btn green">Delete</button></div>
                                       <div class="col-md-3">
                                           <div class="input-group input-medium">
                                               <input id="txtEventId" type="text" class="form-control">
                                               <span class="input-group-btn">
                                                   <button  type="button" id="btnAddSearchEvent" class="btn green">Add</button>
                                               </span>
                                           </div>
                                        </div>
                                       <div class="col-md-2"><button id="btnScanLink" type="button" class="btn green" data-toggle="modal" href="#bulksearch">Search Events</button></div>
                                   </div>
                                   <div class="row">
                                       <div class="col-md-12">
                                           <hr>
                                           <table class="table table-striped table-bordered table-hover" id="table_4">
                                               <thead>
                                                   <tr>
                                                       <th style="display:none"> ID </th>
                                                       <th> Event ID </th>
                                                       <th> Event Title </th>
                                                       <th> Venue </th>
                                                       <th>Event Date</th>
                                                       <th>Active</th>                                                     
                                                   </tr>
                                               </thead>
                                               <tbody>

                                               </tbody>
                                           </table>
                                       </div>
                                   </div>
                               </div>
                               <div class="col-md-4">
                                   <div class="row">
                                       <br>
                                       <br>
                                       <br>
                                       <br>
                                       <div class="col-md-4">
                                           <div>
                                               Search Name:
                                               <input id="Name" type="text" class="form-control input-small" />
                                           </div>
                                           <br>
                                           <div>
                                               Search schedule Date:
                                               <input id="Schedule" type="text" class="form-control input-small date-picker" />
                                           </div>
                                           <br>
                                           <div class="md-checkbox">
                                               <input type="checkbox" id="ScanDayBefore" class="md-check">
                                               <label for="ScanDayBefore">
                                                   <span></span>
                                                   <span class="check"></span>
                                                   <span class="box"></span> Run Once Day
                                               </label>
                                           </div>
                                       </div>
                                       <div class="col-md-6" style="text-align: left">
                                           <br>
                                           <button id="btnAddSearch" type="button"  style="display:block" class="btn green">Add Search</button>
                                           <br>
                                           <button id="btnEditSearch" type="button"  style="display:block" class="btn green">Edit Search</button>
                                           <br>
                                           <button id="btnClearSearchTemp" type="button" style="display:block" class="btn green">Clear Search</button>
                                       </div>
                                   </div>
                               </div>
                            </div>
                        </div>
                    </div>    
                </div>

                <div class="modal fade" id="bulksearch" tabindex="-1" role="basic" aria-hidden="true">
                    <div class="modal-dialog modal-lg">
                        <div class="modal-content">
                            <div class="modal-header" style="background:#5cd1db">
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                                <h4 class="modal-title" style="color:white">Bulk Search</h4>
                            </div>
                            <div class="modal-body">
                                <form action="#" id="newForm" name="newForm" class="form-horizontal">
                                    <div class="form-body">
                                            <div class="form-group">
                                                <label class="control-label col-md-2">
                                                    Event Title
                                                </label>
                                                <div class="col-md-4">
                                                    <input id="txtEventTitle" type="text" class="form-control" name="UserName" required />
                                                </div>
                                                <label class="control-label col-md-1">
                                                    Venue
                                                </label>
                                                <div class="col-md-3">
                                                        <input id="txtVenue" type="text" class="form-control" name="UserName" required />
                                                </div>
                                                <div class="col-md-2">
                                                    <button id="btnSearchEvent" type="button" class="btn green">Search</button>
                                                </div>
                                            </div>
                                      </div>  
                                </form>
                              
                                <div>
                                    <table class="table table-striped table-bordered table-hover" id="bulkeventtable">
                                        <thead>
                                            <tr>
                                                <th></th>
                                                <th>Event ID</th>
                                                <th>Event Title</th>
                                                <th>Venue</th>
                                                <th>Event Date</th>
                                            </tr>
                                        </thead>
                                        <tbody></tbody>
                                    </table>
                                </div>        
                            </div>
                            <div class="modal-footer">
                                <button id="btnSelectAll" type="button" class="btn green">Select All</button>
                                <button id="btnSearchSave" type="button" class="btn green">Save</button>
                                <button id="btnSearchCancel" type="button" class="btn green" data-dismiss="modal">Cancel</button>
                            </div>
                        </div>
                        <!-- /.modal-content -->
                    </div>
                    <!-- /.modal-dialog -->
                </div>
                        
            </div>
        </div>
        <!-- END CONTENT BODY -->
    </div>
    <!-- END CONTENT -->

</div>
<!-- END CONTAINER -->